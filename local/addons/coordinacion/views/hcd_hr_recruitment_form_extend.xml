<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_applicant_view_form_inherit" model="ir.ui.view">
        <field name="name">coordinacion.hr.applicant.view.form.inherit</field>
        <field name="model">hr.applicant</field>
        <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='create_employee_from_applicant']" position="after">
                <button name="create_prestador_from_recruitment" 
                    string="Crear Prestador"
                    type="object"
                    icon="fa-plus"
                    class="btn-primary"/>
            </xpath>
        </field>
    </record>

</odoo>